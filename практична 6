#include <stdio.h>
#include <math.h>

int main() {
    int x1, y1, x2, y2;
    double distance;

    printf("Введіть координати через пробіл: ");
    scanf("%d %d %d %d", &x1, &y1, &x2, &y2);

    // Обчислення довжини вектора
    distance = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));

    // Виведення результату з точністю до шести знаків після коми
    printf("Довжина вектора: %.6lf\n", distance);

    return 0;
}
